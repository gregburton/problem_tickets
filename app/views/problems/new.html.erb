<div class="content">
  <% if logged_in? %>
    <h1>Submit a Problem</h1>
    <p>Please provide a brief description of the problem and what you've tried to solve it so far.</p>
   <div class="formwrap">
     <%= render "problems/problem_form" %>
    </div>
  <% else %>
    <br>
    <h4>Login or <%= link_to "Register", new_user_path %> to submit your problem.</h4>
    <%= render "logins/login_form" %>
<% end %>
</div>