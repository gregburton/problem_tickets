<h4>Login or <%= link_to "Register for a new account", new_user_path %></h4>
<%= form_for(:session, url: logins_path) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.button :submit, class: "btn btn-large btn-success" %>

<% end %>