<div class="content">
  <p></p>
  <h2>Photo Album</h2>
  <p>Where you'll find all our great user submissions.</p>
  <div class="photo_wrap">
    <div class="row">
      <ul class="small-block-grid-3">
        <% @photos.each do |photo| %>
          <li><%= link_to image_tag(photo.image.url(:thumb)), photo %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>